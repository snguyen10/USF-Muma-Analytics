import sys

# Get User Input as a String
shortChar = input()
# Make sure input is >100 Characters
if shortChar.count("") > 100:
    sys.exit
# Create an empty list
new_list = []
# Search List for Upper Case Letters to Add
for letter in shortChar:
    if letter.isupper():
        new_list.append(letter)
# Print the List
print("".join(new_list))
